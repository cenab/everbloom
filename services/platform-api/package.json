{
  "name": "@wedding-bestie/platform-api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "nest start --watch",
    "build": "tsc --build --force",
    "start": "nest start",
    "start:prod": "node dist/main",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "cd ../.. && supabase db push",
    "db:generate": "cd ../.. && supabase migration new",
    "db:seed": "echo 'Run seed SQL via supabase SQL editor or psql'",
    "db:reset": "cd ../.. && supabase db reset",
    "db:reset:remote": "cd ../.. && bash scripts/db-reset-remote.sh",
    "db:reset:prod": "cd ../.. && bash scripts/db-reset-remote.sh"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/throttler": "^6.5.0",
    "@supabase/supabase-js": "^2.39.0",
    "bullmq": "^5.1.0",
    "ioredis": "^5.9.1",
    "jose": "^6.1.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "stripe": "^14.12.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
